{
  "name": "@trop/ioc",
  "version": "2.0.0",
  "description": "Simple IOC container for Javascript",
  "engines": {
    "node": "14"
  },
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha",
    "standardize": "palmito -i '**/tmp/**' -i '**/*.tgz' -i '**/docs/**' -i '**/doc/**'",
    "doc": "jsdoc --destination docs --configure doc/jsdoc.json lib doc/jsdoc_home.md",
    "doc-server": "http-server docs",
    "doc-clean": "rm -rf docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevin-leptons/trop_ioc.git"
  },
  "keywords": [
    "js",
    "ioc",
    "di"
  ],
  "author": "Kevin Leptons",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kevin-leptons/trop_ioc/issues"
  },
  "homepage": "https://github.com/kevin-leptons/trop_ioc#readme",
  "devDependencies": {
    "docdash": "^1.2.0",
    "http-server": "^14.1.0",
    "jsdoc": "^3.6.10",
    "mocha": "8.3.1",
    "palmito": "0.4.0"
  }
}
